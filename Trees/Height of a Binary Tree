 */class Node 
    	int data;
    	Node left;
    	Node right;
	*/
	static int height(Node root) {
        int l=-1; int r =-1;
        if(root!=null)
            {
        if(root.left!=null)
            {l=height(root.left);}
        if(root.right!=null)
            {r=height(root.right);}
        }
      	return l>r?l+1:r+1;
 
            
   
